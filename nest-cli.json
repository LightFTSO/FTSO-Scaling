{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/nest-typescript-starter/apps/nest-typescript-starter/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "plugins": [
      {
        "name": "@nestjs/swagger",
        "options": {
          "classValidatorShim": false,
          "introspectComments": true
        }
      }
    ],
    "webpack": false,
    "tsConfigPath": "apps/nest-typescript-starter/tsconfig.app.json"
  },
  "projects": {
    "ftso-core": {
      "type": "library",
      "root": "libs/ftso-core",
      "entryFile": "index",
      "sourceRoot": "libs/ftso-core/src",
      "compilerOptions": {
        "tsConfigPath": "libs/ftso-core/tsconfig.lib.json"
      }
    },
    "nest-typescript-starter": {
      "type": "application",
      "root": "apps/nest-typescript-starter",
      "entryFile": "main",
      "sourceRoot": "apps/nest-typescript-starter/src",
      "compilerOptions": {
        "tsConfigPath": "apps/nest-typescript-starter/tsconfig.app.json"
      }
    },
    "ftso-calculator": {
      "type": "application",
      "root": "apps/ftso-calculator",
      "entryFile": "main",
      "sourceRoot": "apps/ftso-calculator/src",
      "compilerOptions": {
        "tsConfigPath": "apps/ftso-calculator/tsconfig.app.json"
      }
    },
    "ftso-finalizer": {
      "type": "application",
      "root": "apps/ftso-finalizer",
      "entryFile": "main",
      "sourceRoot": "apps/ftso-finalizer/src",
      "compilerOptions": {
        "tsConfigPath": "apps/ftso-finalizer/tsconfig.app.json"
      }
    },
    "ftso-reward-calculator": {
      "type": "application",
      "root": "apps/ftso-reward-calculator",
      "entryFile": "main",
      "sourceRoot": "apps/ftso-reward-calculator/src",
      "compilerOptions": {
        "tsConfigPath": "apps/ftso-reward-calculator/tsconfig.app.json"
      }
    },
    "top-level-protocol": {
      "type": "application",
      "root": "apps/top-level-protocol",
      "entryFile": "main",
      "sourceRoot": "apps/top-level-protocol/src",
      "compilerOptions": {
        "tsConfigPath": "apps/top-level-protocol/tsconfig.app.json"
      }
    }
  },
  "monorepo": true,
  "root": "apps/nest-typescript-starter"
}