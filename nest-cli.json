{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/ftso-data-provider/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "plugins": [
      {
        "name": "@nestjs/swagger",
        "options": {
          "classValidatorShim": false,
          "introspectComments": true
        }
      }
    ],
    "webpack": false,
    "tsConfigPath": "apps/ftso-data-provider/tsconfig.app.json"
  },
  "projects": {
    "ftso-data-provider": {
      "type": "application",
      "root": "apps/ftso-data-provider",
      "entryFile": "main",
      "sourceRoot": "apps/ftso-data-provider/src",
      "compilerOptions": {
        "tsConfigPath": "apps/ftso-data-provider/tsconfig.app.json"
      }
    },
    "ftso-reward-calculator": {
      "type": "application",
      "root": "apps/ftso-reward-calculator",
      "entryFile": "main",
      "sourceRoot": "apps/ftso-reward-calculator/src",
      "compilerOptions": {
        "tsConfigPath": "apps/ftso-reward-calculator/tsconfig.app.json"
      }
    },
    "example_provider": {
      "type": "application",
      "root": "apps/example_provider",
      "entryFile": "main",
      "sourceRoot": "apps/example_provider/src",
      "compilerOptions": {
        "tsConfigPath": "apps/example_provider/tsconfig.app.json"
      }
    }
  },
  "monorepo": true,
  "root": "apps/ftso-data-provider"
}