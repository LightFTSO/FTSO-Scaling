{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/ftso-calculator/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "plugins": [
      {
        "name": "@nestjs/swagger",
        "options": {
          "classValidatorShim": false,
          "introspectComments": true
        }
      }
    ],
    "webpack": false,
    "tsConfigPath": "apps/ftso-calculator/tsconfig.app.json"
  },
  "projects": {
    "ftso-core": {
      "type": "library",
      "root": "libs/ftso-core",
      "entryFile": "index",
      "sourceRoot": "libs/ftso-core/src",
      "compilerOptions": {
        "tsConfigPath": "libs/ftso-core/tsconfig.lib.json"
      }
    },
    "ftso-calculator": {
      "type": "application",
      "root": "apps/ftso-calculator",
      "entryFile": "main",
      "sourceRoot": "apps/ftso-calculator/src",
      "compilerOptions": {
        "tsConfigPath": "apps/ftso-calculator/tsconfig.app.json"
      }
    },
    "ftso-reward-calculator": {
      "type": "application",
      "root": "apps/ftso-reward-calculator",
      "entryFile": "main",
      "sourceRoot": "apps/ftso-reward-calculator/src",
      "compilerOptions": {
        "tsConfigPath": "apps/ftso-reward-calculator/tsconfig.app.json"
      }
    },
    "example_provider": {
      "type": "application",
      "root": "apps/example_provider",
      "entryFile": "main",
      "sourceRoot": "apps/example_provider/src",
      "compilerOptions": {
        "tsConfigPath": "apps/example_provider/tsconfig.app.json"
      }
    }
  },
  "monorepo": true,
  "root": "apps/ftso-calculator"
}