  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "console": "integratedTerminal",
      "request": "launch",
      "name": "Test EndToEnd",
      "runtimeExecutable": "yarn",
      "outFiles": ["${workspaceFolder}/**/*.js"],
      "env": {
        "NODE_ENV": "development"
      },
      "runtimeArgs": ["hardhat", "test", "test/EndToEnd.test.ts"]
    },
    {
      "type": "node",
      "console": "integratedTerminal",
      "request": "launch",
      "name": "Run price voter",
      "runtimeExecutable": "yarn",
      "outFiles": ["${workspaceFolder}/**/*.js"],
      "env": {
        "NODE_ENV": "development"
      },
      "runtimeArgs": ["ts-node", "deployment/scripts/run-price-voter.ts", "25", "random"]
    },
  ]
}
